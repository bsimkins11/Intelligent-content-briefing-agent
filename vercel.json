{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "backend/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/chat",
      "dest": "/backend/index.py"
    },
    {
      "src": "/upload",
      "dest": "/backend/index.py"
    },
    {
      "src": "/generate-asset",
      "dest": "/backend/index.py"
    },
    {
      "src": "/generate-feed",
      "dest": "/backend/index.py"
    },
    {
      "src": "/brief/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/matrix/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/concepts/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/specs/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/production/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/export/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
